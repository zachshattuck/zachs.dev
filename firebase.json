{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/gallery",
        "destination": "/gallery.html"
      },
      {
        "source": "/gallery/",
        "destination": "/gallery.html"
      }
    ],
    "redirects": [
      {
        "source": "/soundlab",
        "destination": "https://sound.zachs.dev/",
        "type": 301
      },
      {
        "source": "/soundlab/",
        "destination": "https://sound.zachs.dev/",
        "type": 301
      },
      {
        "source": "/sound",
        "destination": "https://sound.zachs.dev/",
        "type": 301
      },
      {
        "source": "/sound/",
        "destination": "https://sound.zachs.dev/",
        "type": 301
      }
    ]
  }
}