{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/gallery",
        "destination": "/gallery.html"
      },
      {
        "source": "/gallery/",
        "destination": "/gallery.html"
      }
    ]
  }
}